{%- comment -%}
  Materials suitability block driven by VDI 3323 metafields

  Expects metafields:
    product.metafields.custom.vdi_excellent  (Integer list)
    product.metafields.custom.vdi_good       (Integer list)
{%- endcomment -%}

{%- assign vdi_excellent = product.metafields.custom.vdi_excellent.value -%}
{%- assign vdi_good = product.metafields.custom.vdi_good.value -%}


{%- if vdi_excellent == blank -%}
  {%- assign vdi_excellent = '' | split: ',' -%}
{%- endif -%}
{%- if vdi_good == blank -%}
  {%- assign vdi_good = '' | split: ',' -%}
{%- endif -%}

{%- comment -%}
  Define VDI groups.
  Format: "start-end::ISO letter::Customer friendly label"
{%- endcomment -%}
{%- assign vdi_groups_raw = 
  "1-5::P::Non alloy steels (mild steels)|"      |
  append: "6-9::P::Low alloy steels|"            |
  append: "10-11::P::High alloy and tool steels|"|
  append: "12-14::M::Stainless steels|"          |
  append: "15-16::K::Grey cast iron|"            |
  append: "17-18::K::Nodular cast iron|"         |
  append: "19-20::K::Malleable cast iron|"       |
  append: "21-22::N::Aluminium. wrought|"        |
  append: "23-25::N::Aluminium. cast|"           |
  append: "26-28::N::Bronze and brass|"          |
  append: "29-30::N::Non metallic materials|"    |
  append: "31-34::S::Heat resistant superalloys|"|
  append: "35-37::S::Titanium alloys|"           |
  append: "38-41::H::Hardened steels and cast irons"
-%}

{%- assign vdi_groups = vdi_groups_raw | split: "|" -%}

<div class="vdi-materials">
  <h3 class="vdi-materials__title">Material suitability</h3>
  <p class="vdi-materials__subtitle">
    Ratings are based on VDI 3323 cutting data.
  </p>

  <div class="vdi-materials__grid">
    {%- for group in vdi_groups -%}
      {%- assign group = group | strip -%}
      {%- if group == '' -%}{% continue %}{%- endif -%}

      {%- assign parts = group | split: "::" -%}
      {%- assign range = parts[0] -%}
      {%- assign iso_letter = parts[1] -%}
      {%- assign label = parts[2] -%}

      {%- assign range_parts = range | split: "-" -%}
      {%- assign start_vdi = range_parts[0] | plus: 0 -%}
      {%- assign end_vdi = range_parts[1] | plus: 0 -%}

      {%- assign excellent_count = 0 -%}
      {%- assign good_count = 0 -%}
      {%- assign total_count = 0 -%}

      {%- for vdi in (start_vdi..end_vdi) -%}
        {%- assign total_count = total_count | plus: 1 -%}
        {%- if vdi_excellent contains vdi -%}
          {%- assign excellent_count = excellent_count | plus: 1 -%}
        {%- elsif vdi_good contains vdi -%}
          {%- assign good_count = good_count | plus: 1 -%}
        {%- endif -%}
      {%- endfor -%}

      {%- assign score = excellent_count | times: 2 | plus: good_count -%}
      {%- assign score_times10 = score | times: 10 | divided_by: total_count -%}

      {%- if score_times10 == 0 -%}
        {%- comment -%}Nothing recommended in this band. Hide row.{%- endcomment -%}
        {%- continue -%}
      {%- endif -%}

      {%- assign rating = '' -%}
      {%- assign rating_class = '' -%}
      {%- assign bar_percent = 0 -%}

      {%- if score_times10 >= 17 -%}
        {%- assign rating = 'Excellent' -%}
        {%- assign rating_class = 'vdi-material-card--excellent' -%}
        {%- assign bar_percent = 100 -%}
      {%- elsif score_times10 >= 12 -%}
        {%- assign rating = 'Great' -%}
        {%- assign rating_class = 'vdi-material-card--great' -%}
        {%- assign bar_percent = 70 -%}
      {%- else -%}
        {%- assign rating = 'Good' -%}
        {%- assign rating_class = 'vdi-material-card--good' -%}
        {%- assign bar_percent = 35 -%}
      {%- endif -%}

      <div class="vdi-material-card {{ rating_class }} vdi-material-card--iso-{{ iso_letter | downcase }}">
        <div class="vdi-material-card__header">
          <span class="vdi-material-card__iso-badge">{{ iso_letter }}</span>
          <span class="vdi-material-card__label">{{ label }}</span>
          <span class="vdi-material-card__rating-text">{{ rating }}</span>
        </div>

        <div class="vdi-material-card__bar">
          <div class="vdi-material-card__bar-track">
            <div class="vdi-material-card__bar-fill" style="width: {{ bar_percent }}%;"></div>
          </div>
          <div class="vdi-material-card__bar-scale">
            <span>Good</span>
            <span>Great</span>
            <span>Excellent</span>
          </div>
        </div>
      </div>
    {%- endfor -%}
  </div>
</div>
