{% if item.tags contains 'Custom Length' and item.metafields.custom.price_matrix %}
  {%- assign raw = item.metafields.custom.price_matrix.value | default: item.metafields.custom.price_matrix -%}
  {%- comment %}
    Flatten everything to spaces, strip HTML, then pick the smallest decimal number.
  {% endcomment -%}
  {%- assign text = raw
      | strip
      | strip_html
      | replace: '\r\n', ' '
      | replace: '\n', ' '
      | replace: '\r', ' '
      | replace: '\t', ' '
  -%}

  {%- comment %} collapse multiple spaces {% endcomment -%}
  {%- assign last = '' -%}
  {%- assign flat = text -%}
  {%- for i in (1..6) -%}
    {%- assign flat = flat | replace: '  ', ' ' -%}
  {%- endfor -%}

  {%- assign tokens = flat | split: ' ' -%}
  {%- assign min_price_cents = nil -%}

  {%- for tok in tokens -%}
    {%- assign s = tok | strip -%}
    {%- if s contains '.' -%}
      {%- comment %}
        Token looks like a decimal price (e.g. 68.24). Convert to cents.
      {% endcomment -%}
      {%- assign cents = s | replace: ',', '.' | times: 100 -%}
      {%- if cents > 0 -%}
        {%- if min_price_cents == nil or cents < min_price_cents -%}
          {%- assign min_price_cents = cents -%}
        {%- endif -%}
      {%- endif -%}
    {%- endif -%}
  {%- endfor -%}

  {%- if min_price_cents -%}
    <span class="from-price">From {{ min_price_cents | money }}</span>
  {%- else -%}
    {%- comment %} Fallback if nothing parsable {% endcomment -%}
    {% include 'price' with wcp_price %}
  {%- endif -%}
{% else %}
  {%- assign price_array = '' -%}
  {%- for v in item.variants -%}
    {%- if v.price > 0 -%}
      {%- assign price_array = price_array | append: v.price | append: ',' -%}
    {%- endif -%}
  {%- endfor -%}

  {%- assign price_array = price_array | split: ',' | compact -%}

  {% if price_array.size == 0 %}
    {% comment %} All prices are 0 → hide price {% endcomment %}
  {% else %}
    {%- assign sorted_prices = price_array | sort: "number" -%}
    {%- assign min_price = sorted_prices | first | plus: 0 -%}
    {%- assign max_price = sorted_prices | last  | plus: 0 -%}

    {% if min_price == max_price %}
      {% include 'price' with min_price %}
    {% else %}
      <span class="from-to-price">
        {{ min_price | money }} – {{ max_price | money }}
      </span>
    {% endif %}
  {% endif %}
{% endif %}


