<div class="payment-buttons payment-buttons--medium form-payment-buttons">
  {%- assign all_zero = true -%}
  {%- assign addable_id = blank -%}

  {%- for v in item.variants -%}
    {%- if v.price > 0 -%}
      {%- assign all_zero = false -%}
    {%- endif -%}
  {%- endfor -%}

  {%- comment -%} Find an addable variant {% endcomment -%}
  {%- for v in item.variants -%}
    {%- if v.price > 0 -%}
      {%- if v.inventory_quantity > 0 -%}
        {%- assign addable_id = v.id -%}{%- break -%}
      {%- elsif v.inventory_policy == 'continue' -%}
        {%- assign addable_id = v.id -%}{%- break -%}
      {%- endif -%}
    {%- endif -%}
  {%- endfor -%}

  {% if item.tags contains 'Custom Length' %}
    <a href="{{ item.url }}" class="btn btn--wide btn--buyNow" style="background:#f88d2b;color:#fff;">
      <span style="display:flex;align-items:center;gap:10px;">Select your blade</span>
    </a>

  {% elsif all_zero %}
    <a href="{{ item.url }}" class="btn btn--wide btn--buyNow is-quote" style="background:#f88d2b;color:#fff;">
      <span style="display:flex;align-items:center;gap:10px;">Request Quote</span>
    </a>

  {% elsif addable_id %}
    <a href="javascript:void(0);" class="btn btn--wide btn--buyNow add-to-cart-btn"
       data-variant-id="{{ addable_id }}" style="background:#e0592a;color:#fff;">
      <span style="display:flex;align-items:center;gap:10px;">
        <i class="fa fa-cart" style="content:url(https://cdn.shopify.com/s/files/1/0439/5597/8399/files/Asset_Cart-white.png?v=1730169426);height:17px;"></i>
        Add to Cart
      </span>
    </a>

  {% else %}
    <a href="{{ item.url }}" class="btn btn--wide" style="background:#e8e8e8;color:#222;">
      <span style="display:flex;align-items:center;gap:10px;">Sold out</span>
    </a>
  {% endif %}
</div>




